<h1>Projects:</h1>

<% if @projects.any? %>
  <ul>
    <% @projects.each do |project| %>
      <li><%= link_to project.title, project_path(project) %></li>
    <% end %>
  </ul>
<% else %>
  <p>There are no projects yet.</p>
<% end %>

<p><%= link_to "Add a project", new_project_path %></p>

<p><%= link_to "Divisions", divisions_path %></p>
